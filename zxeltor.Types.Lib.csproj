<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>zxeltor.Types.Lib</Title>
    <Description>A general purpose library of types and helpers</Description>
    <PackageProjectUrl>https://github.com/zxeltor/zxeltor.Types.Lib</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryType>git</RepositoryType>
    <PackageIcon>LLAP128x128.png</PackageIcon>
    <RepositoryUrl>https://github.com/zxeltor/zxeltor.Types.Lib</RepositoryUrl>
    <Authors>Todd Taylor</Authors>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <Version>1.1.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="log4net" Version="3.0.1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

  <ItemGroup>
    <None Update="LLAP128x128.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Update="README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
